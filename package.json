{
  "name": "excel-custom-functions",
  "version": "1.0.0",
  "main": "./src/customfunctions.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "start npm run dev-server && start npm run watch && npm run sideload",
    "start-web": "start npm run dev-server && start npm run watch",
    "build": "webpack --mode development && webpack --mode development --config webpack.win32.config.js",
    "dev-server": "webpack-dev-server --content-base dist/ --mode development --https --key ./certs/server.key --cert ./certs/server.crt --cacert ./certs/ca.crt --hot --inline",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "watch": "webpack --mode development --watch"
  },
  "devDependencies": {
    "@types/custom-functions-runtime": "^1.4.0",
    "@types/office-runtime": "^1.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "office-toolbox": "^0.1.0",
    "ts-loader": "^4.4.2",
    "tslib": "^1.9.3",
    "typescript": "^2.7.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  }
}
