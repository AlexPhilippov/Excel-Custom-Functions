{
  "name": "excel-custom-functions",
  "version": "1.0.0",
  "scripts": {
    "start": "start npm run dev-server && npm run watch && npm run sideload",
    "start-web": "start npm run dev-server && npm run watch",
    "build": "npm run build:shared && npm run build:native",
    "build:shared": "webpack --mode production --config webpack.config.shared.js",
    "build:native": "webpack --mode production --config webpack.config.native.js",
    "dev-server": "webpack-dev-server --config webpack.config.shared.js --content-base dist/ --mode development --https --key ./certs/server.key --cert ./certs/server.crt --cacert ./certs/ca.crt --hot --inline",
    "watch": "start npm run watch:shared && start npm run watch:native",
    "watch:shared": "webpack --mode development --config webpack.config.shared.js --watch",
    "watch:native": "webpack --mode development --config webpack.config.native.js --watch",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel"
  },
  "devDependencies": {
    "@types/custom-functions-runtime": "^1.4.0",
    "@types/office-runtime": "^1.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "office-toolbox": "^0.1.0",
    "ts-loader": "^4.4.2",
    "tslib": "^1.9.3",
    "typescript": "^2.7.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  }
}
